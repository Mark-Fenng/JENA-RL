prefix ub: <http://www.lehigh.edu/~zhp2/2004/0401/univ-bench.owl#>
select ?x ?y ?z {
  ?x a ub:GraduateStudent ;
    ub:undergraduateDegreeFrom ?y ;
    ub:memberOf ?z .
  ?y a ub:University .
  ?z a ub:Department ;
    ub:subOrganizationOf ?y
}